{
  "name": "sapphire_backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": "Abhigya",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig-build.json && tsc-alias -p tsconfig-build.json",
    "build:watch": "tsc -p tsconfig-build.json && (concurrently \"tsc -p tsconfig-build.json -w\" \"tsc-alias -p tsconfig-build.json -w\")",
    "start": "node -r dotenv/config dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear -r dotenv/config -r tsconfig-paths/register  src/server.ts",
    "clean": "rimraf dist",
    "codegen": "kysely-codegen --dialect postgres --numeric-parser number --log-level debug --env-file .env --out-file ./src/database/db.d.ts",
    "codegen:no-verbose": "kysely-codegen --dialect postgres --numeric-parser number --log-level warning --env-file .env --out-file ./src/database/db.d.ts",
    "lint": "tslint --project tsconfig.json",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js)\" --write",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.0.0",
    "@aws-sdk/s3-request-presigner": "3.0.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^8.5.1",
    "express-winston": "^4.2.0",
    "express-ws": "^5.0.2",
    "fast-xml-parser": "^5.2.5",
    "firebase-admin": "^13.4.0",
    "formdata-node": "^6.0.3",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.0.0",
    "i18n-iso-countries": "^7.14.0",
    "joi": "^17.13.3",
    "joi-to-swagger": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^7.0.3",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.2",
    "qrcode": "^1.5.4",
    "redis": "^5.5.6",
    "request-ip": "^3.3.0",
    "speakeasy": "^2.0.0",
    "stoppable": "^1.1.0",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.29.0",
    "@jest/globals": "^30.0.2",
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/csurf": "^1.11.5",
    "@types/express": "^5.0.3",
    "@types/express-jwt": "^7.4.4",
    "@types/express-winston": "^4.0.2",
    "@types/express-ws": "^3.0.5",
    "@types/geoip-lite": "^1.4.4",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^24.0.4",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.4",
    "@types/qrcode": "^1.5.5",
    "@types/redis": "^4.0.11",
    "@types/request-ip": "^0.0.41",
    "@types/speakeasy": "^2.0.10",
    "@types/stoppable": "^1.1.3",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "babel-jest": "^30.0.2",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "kysely-codegen": "^0.18.5",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.0",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.8.2"
  }
}
